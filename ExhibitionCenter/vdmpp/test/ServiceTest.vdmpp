class ServiceTest  is subclass of Test_vdm
operations
	public testAll: () ==> ()
	testAll() == (
		dcl service : Service := new Service(10,<IT>);
		testSetPrice(service, 20)
	);
	-- Set Conditions
	public testSetPrice: Service * real ==> ()
	testSetPrice(s, pr) == (
		s.setPrice(pr);
		assertTrue(s.price = pr);
	);
end ServiceTest